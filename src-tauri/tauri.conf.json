{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "dmbar",
  "version": "0.1.15",
  "identifier": "com.dmbar.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:8081",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "dmbar",
        "width": 354,
        "height": 566,
        "resizable": true,
        "center": true,
        "decorations": false,
        "transparent": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "resources"
    ]
  }
}
